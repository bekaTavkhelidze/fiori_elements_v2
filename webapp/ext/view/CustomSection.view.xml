			<Label labelFor="idGeorgiaCountryText" text="Country:"></Label>
			<Text id="idGeorgiaCountryText" text="Georgia"></Text>
		</layout:VerticalLayout>
		<layout:VerticalLayout>

			<Label labelFor="idTbilisiCityText" text="City:"></Label>
			<Text id="idTbilisiCityText" text="Tbilisi"></Text>
		</layout:VerticalLayout>
